# AI Provider Configuration
# Options: openai, anthropic, local, mock
AI_PROVIDER=mock

# API Keys (required for non-mock providers)
# For OpenAI
AI_API_KEY=your-openai-api-key-here
AI_MODEL=gpt-4

# For Anthropic (when implemented)
# AI_API_KEY=your-anthropic-api-key-here
# AI_MODEL=claude-3-opus-20240229

# Optional: Custom API endpoint
# AI_BASE_URL=https://api.openai.com/v1

# Agent Configuration
AGENT_NAME=IntelligentAgent
LOG_LEVEL=info

# Performance Settings
MAX_CONCURRENT_TASKS=5
TASK_TIMEOUT=60000

# Memory Settings
MAX_MEMORIES=10000
MEMORY_CONSOLIDATION_INTERVAL=3600000