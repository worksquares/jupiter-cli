{
  "templates": [
    {
      "id": "react",
      "name": "React Starter Template",
      "description": "A modern React application with TypeScript, Tailwind CSS, shadcn/ui components, and Lucide icons",
      "framework": "react",
      "language": "typescript",
      "features": [
        "React 18",
        "TypeScript",
        "React Router",
        "Tailwind CSS",
        "shadcn/ui components",
        "Lucide React icons",
        "Web Vitals"
      ],
      "buildCommand": "npm run build",
      "startCommand": "npm start",
      "outputDirectory": "build"
    },
    {
      "id": "vue",
      "name": "Vue 3 Starter Template",
      "description": "A Vue 3 application with Composition API, TypeScript, Tailwind CSS, and Lucide icons",
      "framework": "vue",
      "language": "typescript",
      "features": [
        "Vue 3",
        "TypeScript",
        "Vue Router",
        "Vuex",
        "Composition API",
        "Tailwind CSS",
        "Lucide Vue icons"
      ],
      "buildCommand": "npm run build",
      "startCommand": "npm run serve",
      "outputDirectory": "dist"
    },
    {
      "id": "angular",
      "name": "Angular Starter Template",
      "description": "An Angular 17 application with standalone components, Tailwind CSS, and Lucide icons",
      "framework": "angular",
      "language": "typescript",
      "features": [
        "Angular 17",
        "TypeScript",
        "Standalone Components",
        "Angular Router",
        "RxJS",
        "Tailwind CSS",
        "Lucide Angular icons"
      ],
      "buildCommand": "npm run build",
      "startCommand": "npm start",
      "outputDirectory": "dist/angular-starter-template"
